{
  "name": "vscode-flutter-insert-remove-helper",
  "displayName": "Flutter Insert and Remove",
  "description": "This extension currently Help you quickly insert and remove Widget in fluttter, no need to selection, select the corresponding Widget right click can be",
  "version": "0.1.7",
  "publisher": "evils",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/EvStorM/vscode-flutter-insert-remove-helper.git"
  },
  "engines": {
    "vscode": "^1.76.1"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "dart",
    "flutter",
    "insert",
    "remove",
    "node",
    "child",
    "parent",
    "sibling",
    "Widget"
  ],
  "activationEvents": [
    "workspaceContains:**/pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "extensionDependencies": [
    "Dart-Code.dart-code"
  ],
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "flutterRiverpodHelper.insertParent",
          "when": "editorLangId == dart",
          "title": "insert a Parent Widget"
        },
        {
          "command": "flutterRiverpodHelper.removeParent",
          "when": "editorLangId == dart",
          "title": "remove a Parent Widget"
        },
        {
          "command": "flutterRiverpodHelper.insertSibling",
          "when": "editorLangId == dart",
          "title": "insert a Parent and Sibling Widget"
        }
      ]
    },
    "commands": [
      {
        "command": "flutterRiverpodHelper.insertParent",
        "when": "editorLangId == dart",
        "group": "1_modification",
        "title": "insert a Parent Widget"
      },
      {
        "command": "flutterRiverpodHelper.removeParent",
        "when": "editorLangId == dart",
        "group": "1_modification",
        "title": "remove a Parent Widget"
      },
      {
        "command": "flutterRiverpodHelper.insertSibling",
        "when": "editorLangId == dart",
        "group": "1_modification",
        "title": "insert a Parent and Sibling Widget"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^4.9.5",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "tree-kill": "^1.2.1",
    "xregexp": "^5.1.1"
  }
}
